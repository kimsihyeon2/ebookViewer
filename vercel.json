{
    "routes": [
        { "src": "/manifest.json", "dest": "/manifest.json" },
        { "src": "/(.*)", "dest": "/index.html" }
    ],
    "build": {
      "env": {
        "NPM_FLAGS": "--prefer-offline"
      }
    },
    "env": {
      "REACT_APP_API_URL": "https://ebookviewer-production.up.railway.app"
    }
  }